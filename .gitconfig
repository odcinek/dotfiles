[user]
  email = odcinek@gmail.com
#  email = odcinek@fb.com
  name = Marcin Sawicki
[push]
  default = matching
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red
  new = cyan
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
#[http]
#  proxy = fwdproxy.any:8080
[url "https://github.com/"]
  insteadOf = git://github.com/
[alias]
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
